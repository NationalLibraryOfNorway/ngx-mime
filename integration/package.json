{
  "name": "integration-test",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf aot/ dist/ node_modules/@nationallibraryofnorway .tmp/ && mkdir .tmp",
    "ng": "ng",
    "start": "ng serve --prod --no-progress --port 8080",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "mountebank": "mb --port 2020 --configfile ./e2e/templates/imposters.ejs --allowInjection --nologfile",
    "e2e:all": "concurrently \"npm run mountebank\" \"npm run start\" \"protractor:desktop && protractor:mobile\" --kill-others --success first",
    "e2e:chrome": "concurrently \"npm run mountebank\" \"npm run start\" \"npm run protractor:chrome:headless\" --kill-others --success first",
    "e2e:firefox": "concurrently \"npm run mountebank\" \"npm run start\" \"npm run protractor:firefox\" --kill-others --success first",
    "wait": "sh ./scripts/wait-on.sh",
    "preprotractor": "webdriver-manager update && npm run wait",
    "protractor": "protractor ./e2e/protractor.conf.js",
    "preprotractor:chrome:headless": "npm run preprotractor",
    "protractor:chrome:headless": "npm run protractor --  --browser chrome --tags @desktop --headless",
    "preprotractor:firefox": "npm run preprotractor",
    "protractor:firefox": "npm run protractor -- --browser firefox --tags @desktop",
    "protractor:desktop": "npm run protractor -- --device=desktop",
    "protractor:mobile": "npm run protractor:android",
    "protractor:android": "npm run protractor --  --device=android",
    "protractor:iphone": "npm run protractor --  --device=iphone"    
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.2",
    "@angular/cdk": "6.0.2",
    "@angular/common": "6.0.2",
    "@angular/compiler": "6.0.2",
    "@angular/core": "6.0.2",
    "@angular/flex-layout": "6.0.0-beta.15",
    "@angular/forms": "6.0.2",
    "@angular/http": "6.0.2",
    "@angular/material": "6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/platform-browser-dynamic": "6.0.2",
    "@angular/router": "6.0.2",
    "@nationallibraryofnorway/ngx-mime": "../dist/ngx-mime",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.5.5",
    "d3": "4.1.0",
    "openseadragon": "2.3.1",
    "rxjs": "6.1.0",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.1",
    "@angular/compiler-cli": "6.0.2",
    "@angular-devkit/build-angular": "0.6.1",
    "@angular/language-service": "6.0.2",
    "@types/chai": "4.1.2",
    "@types/chai-as-promised": "7.1.0",
    "@types/cucumber": "2.0.4",
    "@types/fs-extra": "5.0.2",
    "@types/jasmine": "2.8.6",
    "@types/node": "9.6.5",
    "@types/es6-promise": "3.3.0",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chromedriver": "2.37.0",
    "codelyzer": "4.2.1",
    "concurrently": "3.5.1",
    "cucumber": "4.2.1",
    "express": "4.16.3",
    "fs-extra": "5.0.0",
    "geckodriver": "1.11.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "jsonfile": "4.0.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "1.0.0",
    "mountebank": "1.14.0",
    "protractor": "5.3.1",
    "protractor-cucumber-framework": "5.0.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "1.7.0",
    "rimraf": "2.6.2",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  }
}
