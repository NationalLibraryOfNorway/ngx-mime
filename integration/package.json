{
  "name": "integration-test",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf aot/ dist/ node_modules/@nationallibraryofnorway .tmp/",
    "ng": "ng",
    "start": "ng build --prod --no-progress && node server/index.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "mountebank": "mb --port 2020 --configfile ./e2e/templates/imposters.ejs --allowInjection --nologfile",
    "e2e:all": "concurrently \"npm run mountebank\" \"npm run start\" \"npm run protractor\" --kill-others --success first",
    "e2e:chrome": "concurrently \"npm run mountebank\" \"npm run start\" \"npm run protractor:chrome -- --headless\" --kill-others --success first",
    "wait": "sh ./scripts/wait-on.sh",
    "preprotractor": "webdriver-manager update --versions.standalone=3.4.0",
    "protractor": "npm run wait && concurrently \"npm run protractor:chrome\" \"npm run protractor:firefox\" \"npm run protractor:edge\" \"npm run protractor:android:chrome\"",
    "protractor:chrome": "npm run wait && protractor --browser=chrome --tags=@desktop > .tmp/chrome_result.txt && cat .tmp/chrome_result.txt",
    "protractor:firefox": "npm run wait && protractor --browser=firefox --tags=@desktop > .tmp/firefox_result.txt && cat .tmp/firefox_result.txt",
    "protractor:edge": "npm run wait && protractor --browser=MicrosoftEdge --tags=@desktop > .tmp/edge_result.txt && cat .tmp/edge_result.txt",
    "protractor:ie": "npm run wait && protractor --browser=\"internet explorer\" --tags=@desktop > .tmp/ie_result.txt && cat .tmp/ie_result.txt",
    "protractor:safari": "npm run wait && protractor --browser=safari --tags=@desktop > .tmp/safari_result.txt && cat .tmp/safari_result.txt",
    "protractor:android:chrome": "npm run wait && protractor --browser=Chrome --tags=@android > .tmp/android_chrome_result.txt && cat .tmp/android_chrome_result.txt",
    "protractor:ios:safari": "npm run wait && protractor --browser=Safari --tags=@iphone > .tmp/ios_safari_result.txt && cat .tmp/ios_safari_result.txt"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.4.4",
    "@angular/cdk": "2.0.0-beta.12",
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/flex-layout": "2.0.0-beta.9",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/material": "2.0.0-beta.12",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@angular/router": "4.4.4",
    "@nationallibraryofnorway/ngx-mime": "../dist/",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "d3": "^4.10.0",
    "openseadragon": "2.3.1",
    "rxjs": "^5.1.0",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.4.5",
    "@angular/compiler-cli": "4.4.4",
    "@angular/language-service": "4.4.4",
    "@types/chai": "3.5.2",
    "@types/chai-as-promised": "0.0.30",
    "@types/cucumber": "2.0.1",
    "@types/fs-extra": "3.0.2",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codelyzer": "~3.0.1",
    "concurrently": "^3.4.0",
    "cucumber": "2.3.1",
    "fs-extra": "3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jsonfile": "3.0.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mountebank": "^1.11.0",
    "multiple-cucumber-html-reporter": "0.1.0",
    "protractor": "~5.1.2",
    "protractor-cucumber-framework": "3.1.0",
    "rimraf": "^2.5.4",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "chromedriver": "2.33.1"
  }
}
