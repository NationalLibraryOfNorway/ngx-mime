{
  "name": "integration-test",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf aot/ dist/ node_modules/@nationallibraryofnorway .tmp/ && mkdir .tmp",
    "ng": "ng",
    "start": "ng build --aot -sm -ec --no-progress && node server/index.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "mountebank": "mb --port 2020 --configfile ./e2e/templates/imposters.ejs --allowInjection --nologfile",
    "e2e:all": "concurrently \"npm run mountebank\" \"npm run start\" \"npm run protractor\" --kill-others --success first",
    "e2e:chrome": "concurrently \"npm run mountebank\" \"npm run start\" \"npm run protractor:chrome:headless\" --kill-others --success first",
    "e2e:firefox": "concurrently \"npm run mountebank\" \"npm run start\" \"npm run protractor:firefox\" --kill-others --success first",
    "wait": "sh ./scripts/wait-on.sh",
    "preprotractor": "webdriver-manager update --versions.standalone=3.4.0",
    "protractor": "npm run wait && concurrently \"npm run protractor:chrome\" \"npm run protractor:firefox\" \"npm run protractor:edge\" \"npm run protractor:android:chrome\"",
    "protractor:chrome": "./scripts/protractor.sh --browser chrome --tags @desktop",
    "protractor:chrome:headless": "./scripts/protractor-chrome-headless.sh",
    "protractor:firefox": "./scripts/protractor.sh --browser firefox --tags @desktop",
    "protractor:edge": "./scripts/protractor.sh --browser MicrosoftEdge --tags @desktop",
    "protractor:ie": "./scripts/protractor.sh --browser \"internet explorer\" --tags @desktop",
    "protractor:safari": "./scripts/protractor.sh --browser safari --tags @desktop",
    "protractor:android:chrome": "./scripts/protractor.sh --browser Chrome --tags @android",
    "protractor:ios:safari": "./scripts/protractor.sh --browser Safari --tags @iphone"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.0",
    "@angular/cdk": "5.0.0-rc0",
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/flex-layout": "2.0.0-beta.10-4905443",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/material": "5.0.0-rc0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@angular/router": "5.0.0",
    "@nationallibraryofnorway/ngx-mime": "../dist/",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "d3": "^4.10.0",
    "openseadragon": "2.3.1",
    "rxjs": "5.5.2",
    "web-animations-js": "^2.2.5",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/language-service": "5.0.0",
    "@types/chai": "4.0.4",
    "@types/chai-as-promised": "0.0.30",
    "@types/cucumber": "2.0.4",
    "@types/fs-extra": "4.0.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chromedriver": "2.33.2",
    "codelyzer": "3.2.0",
    "concurrently": "^3.4.0",
    "cucumber": "^3.1.0",
    "express": "^4.16.2",
    "fs-extra": "3.0.1",
    "geckodriver": "1.9.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jsonfile": "3.0.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mountebank": "^1.11.0",
    "protractor": "5.2.0",
    "protractor-cucumber-framework": "4.1.1",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.2.3",
    "rimraf": "^2.5.4",
    "ts-node": "~3.0.4",
    "tslint": "5.7.0",
    "typescript": "2.4.2"
  }
}
